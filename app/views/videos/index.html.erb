<div class="container">
  <% @videos.each do |v| %>
    <div class="category_item">
      <iframe id="player" type="text/html" width="100%" height="222px;" src="http://www.youtube.com/embed/<%= v.video_id %>" frameborder="0"></iframe>
      <%= render 'video_info', :v => v %>
    </div>
  <% end %>	
</div>